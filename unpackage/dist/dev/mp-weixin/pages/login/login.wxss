.login .logo {
  width: 97rpx;
  height: 74rpx;
  margin: 132rpx auto 114rpx;
  background: blue;
}
.login .logo image {
    width: 100%;
    height: 100%;
}
.login .main {
  width: 593rpx;
  height: 795rpx;
  background: white;
  border: 1rpx solid white;
  -webkit-box-shadow: 8rpx 9rpx 35rpx 0px rgba(164, 78, 153, 0.25);
          box-shadow: 8rpx 9rpx 35rpx 0px rgba(164, 78, 153, 0.25);
  border-radius: 0px 50rpx 0px 50rpx;
  margin: 0 auto;
  padding: 0 86rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.login .main .title {
    font-size: 35rpx;
    font-family: FZZHJW--GB1-0;
    font-weight: bold;
    color: #8066ff;
    text-align: center;
    margin-top: 53rpx;
}
.login .main .input {
    font-size: 30rpx;
    font-family: PingFang-SC-Light;
    font-weight: bold;
    color: #cabfff;
}
.login .main .input view {
      margin-top: 88rpx;
}
.login .main .input ._input {
      border-bottom: 1rpx solid #cabfff;
      padding: 0 0 23rpx 52rpx;
      width: 420rpx;
      -webkit-box-sizing: border-box;
              box-sizing: border-box;
}
.login .main .login-btn {
    width: 420rpx;
    height: 63rpx;
    background: #8066ff;
    border-radius: 32rpx;
    font-size: 30rpx;
    font-family: FZZHJW--GB1-0;
    font-weight: bold;
    color: #fefeff;
    line-height: 74rpx;
    margin: 76rpx auto 0;
    text-align: center;
    line-height: 63rpx;
}
.login .main .tips {
    font-size: 24rpx;
    font-family: PingFang-SC-Light;
    font-weight: bold;
    color: #ffadb9;
    margin-top: 41rpx;
    text-align: center;
}
.login .main .icon {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    margin-top: 70rpx;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
        -ms-flex-pack: justify;
            justify-content: space-between;
    padding: 0 110rpx;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
}
.login .main .icon text {
      width: 60rpx;
      height: 60rpx;
      background: blue;
      border-radius: 50%;
}
.login .footer-protocol {
  font-size: 20rpx;
  font-family: FZZHJW--GB1-0;
  font-weight: bold;
  color: #cabfff;
  text-align: center;
  margin-top: 94rpx;
}
.login .footer-protocol navigator {
    display: inline-block;
    color: #2600E1;
}

