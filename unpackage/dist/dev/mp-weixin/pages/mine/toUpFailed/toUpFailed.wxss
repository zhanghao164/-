.content {
  padding: 190rpx 49rpx 0 49rpx;
}
.content .icon {
    width: 473rpx;
    height: 321rpx;
    margin: 0 auto;
    background: blue;
}
.content .icon image {
      width: 100%;
      height: 100%;
}
.content .title {
    font-size: 32rpx;
    font-family: PingFang-SC-Medium;
    font-weight: bold;
    color: #474747;
    text-align: center;
    margin: 98rpx 0 70rpx 0;
}
.content .addMoneyInfo view {
    height: 58rpx;
    line-height: 58rpx;
    font-size: 28rpx;
    font-family: PingFang-SC-Medium;
    font-weight: bold;
    color: #474747;
}
.content .addMoneyInfo view text:nth-child(2) {
      float: right;
      font-family: PingFang-SC-Light;
}
.content .toHome-btn {
    width: 300rpx;
    height: 80rpx;
    background: #8066ff;
    -webkit-box-shadow: 0px 17rpx 32rpx 0px rgba(164, 128, 255, 0.77);
            box-shadow: 0px 17rpx 32rpx 0px rgba(164, 128, 255, 0.77);
    border-radius: 40rpx;
    font-size: 28rpx;
    font-family: PingFang-SC-Medium;
    font-weight: bold;
    color: white;
    margin: 72rpx auto 0;
    line-height: 80rpx;
    text-align: center;
}

